<?xml version="1.0" encoding="utf-8"?>
    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="net.octword.everest.field.MainActivity">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="262dp"
        android:layout_height="21dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="60dp"
        android:text="Informacion de Registro"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
        <TextView_toTopOf="parent" />


    <TableLayout
        android:id="@+id/tableLayout2"
        android:layout_width="362dp"
        android:layout_height="578dp"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="121dp">

        <TableRow
            android:padding="5dp"
            android:shrinkColumns="*"
            android:stretchColumns="*">

        </TableRow>

        <TableRow
            android:layout_width="293dp"
            android:padding="5dp"
            android:shrinkColumns="*"
            android:stretchColumns="*">

            <EditText
                android:id="@+id/EditText_Nombre"
                android:layout_width="282dp"
                android:layout_height="40dp"
                android:ems="10"
                android:hint="Nombre"
                android:drawableStart="@drawable/user_icon_24px"
                android:inputType="text"
                android:background="@drawable/customize_editext"
                android:paddingStart="10dp"
                android:drawablePadding="10dp"
                android:textSize="14sp" />
        </TableRow>

        <TableRow
            android:padding="5dp"
            android:shrinkColumns="*"
            android:stretchColumns="*">

            <EditText
                android:id="@+id/EditText_Apellido"
                android:layout_width="282dp"
                android:layout_height="40dp"

                android:ems="10"
                android:hint="Apellido"
                android:drawableStart="@drawable/user_icon_24px"
                android:background="@drawable/customize_editext"
                android:paddingStart="10dp"
                android:drawablePadding="10dp"
                android:inputType="text"
                android:textSize="14sp" />
        </TableRow>

        <TableRow
            android:padding="5dp"
            android:shrinkColumns="*"
            android:stretchColumns="*">

            <EditText
                android:id="@+id/EditText_Identificacion"
                android:layout_width="120dp"
                android:layout_height="40dp"
                android:drawableStart="@drawable/id_icon32px"
                android:background="@drawable/customize_editext"
                android:paddingStart="10dp"
                android:drawablePadding="10dp"
                android:ems="10"
                android:hint="Identificacion"

                android:inputType="number"
                android:textSize="14sp" />
        </TableRow>

        <TableRow
            android:padding="5dp"
            android:shrinkColumns="*"
            android:stretchColumns="*">

            <MultiAutoCompleteTextView
                android:id="@+id/MultiAutoC_Sexo"
                android:layout_width="120dp"
                android:layout_height="40dp"
                android:drawableStart="@drawable/sex_icon_24px"
                android:background="@drawable/customize_editext"
                android:paddingStart="10dp"
                android:drawablePadding="10dp"
                android:ems="10"
                android:hint="Sexo"
                android:textSize="14sp" />


        </TableRow>

        <TableRow
            android:padding="5dp"
            android:shrinkColumns="*"
            android:stretchColumns="*">

            <EditText
                android:id="@+id/EditText_DigitoVerificador"
                android:layout_width="120dp"
                android:layout_height="40dp"
                android:drawableStart="@drawable/numeral_icon_24px"
                android:background="@drawable/customize_editext"
                android:paddingStart="10dp"
                android:drawablePadding="10dp"
                android:ems="10"
                android:hint="Digito Verificador"
                android:inputType="number"
                android:textSize="14sp" />
        </TableRow>

        <TableRow
            android:padding="5dp"
            android:shrinkColumns="*"
            android:stretchColumns="*">

            <EditText
                android:id="@+id/EditText_FechaNacimiento"
                android:layout_width="120dp"
                android:layout_height="40dp"
                android:drawableStart="@drawable/calendar_icon_24px"
                android:background="@drawable/customize_editext"
                android:paddingStart="10dp"
                android:drawablePadding="10dp"
                android:ems="10"
                android:hint="Fecha de Nacimiento"
                android:inputType="date"
                android:textSize="14sp" />
        </TableRow>

        <TableRow
            android:layout_height="60dp"
            android:padding="5dp"
            android:shrinkColumns="*"
            android:stretchColumns="*">

            <EditText
                android:id="@+id/EditText_TelefoFijo"
                android:layout_width="120dp"
                android:layout_height="40dp"
                android:drawableStart="@drawable/call_icon_24px"
                android:background="@drawable/customize_editext"
                android:paddingStart="10dp"
                android:drawablePadding="10dp"
                android:ems="10"
                android:hint="Telefono Fijo"
                android:inputType="number"
                android:textSize="14sp" />
        </TableRow>

        <TableRow
            android:padding="5dp"
            android:shrinkColumns="*"
            android:stretchColumns="*">

            <EditText
                android:id="@+id/EditText_AmountPerCycle"
                android:layout_width="120dp"
                android:layout_height="40dp"
                android:drawableStart="@drawable/cell_icon_24px"
                android:background="@drawable/customize_editext"
                android:paddingStart="10dp"
                android:drawablePadding="10dp"
                android:ems="10"
                android:hint="Telefono Celular"
                android:inputType="number"
                android:textSize="14sp" />
        </TableRow>

        <TableRow
            android:padding="5dp"
            android:shrinkColumns="*"
            android:stretchColumns="*">

            <EditText
                android:id="@+id/EditText_Email"
                android:layout_width="120dp"
                android:layout_height="40dp"
                android:drawableStart="@drawable/email_icon_24px"
                android:background="@drawable/customize_editext"
                android:paddingStart="10dp"
                android:drawablePadding="10dp"
                android:ems="10"
                android:hint="E-mail"
                android:inputType="textEmailAddress"
                android:textSize="14sp" />
        </TableRow>

        <TableRow
            android:padding="5dp"
            android:shrinkColumns="*"
            android:stretchColumns="*">

            <EditText
                android:id="@+id/EditText_Pais"
                android:layout_width="120dp"
                android:layout_height="40dp"
                android:drawableStart="@drawable/earth_icon_24px"
                android:background="@drawable/customize_editext"
                android:paddingStart="10dp"
                android:drawablePadding="10dp"
                android:ems="10"
                android:hint="Pais"
                android:inputType="textEmailAddress"
                android:textSize="14sp" />
        </TableRow>
    </TableLayout>

    <Button
        android:id="@+id/button"
        android:layout_width="246dp"
        android:layout_height="49dp"
        android:background="@drawable/customize_btn"
        android:text="Guardar"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="64dp"
        tools:layout_editor_absoluteY="702dp" />


</androidx.constraintlayout.widget.ConstraintLayout>